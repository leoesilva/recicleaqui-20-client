{
  "expo": {
    "name": "recicleaqui-20-client",
    "slug": "recicleaqui-20-client",
    "version": "1.0.0",
    "scheme": "recicleaqui20client",
    "orientation": "portrait",
    "icon": "./assets/images/logo-recicle-aqui.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo-recicle-aqui.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.caiocesardev.recicleaqui20client"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-recicle-aqui.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.caiocesardev.recicleaqui20client"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font"
      ]
    ]
  }
}
